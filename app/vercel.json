{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/changelog", "destination": "/changelog" },
    { "source": "/roadmap", "destination": "/roadmap" },
    { "source": "/blog", "destination": "/blog" },
    { "source": "/blog/:post", "destination": "/blog/:post" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
